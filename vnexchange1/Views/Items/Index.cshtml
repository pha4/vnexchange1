@model PaginatedList<vnexchange1.Models.Item>
@using vnexchange1.Data;
@using vnexchange1.PaginatedList;
@using System.Globalization;
@{
    ViewData["Title"] = "Danh muc";
}

<h2>
    @ViewBag.Category
</h2>
@if (Model.Count() == 0)
{
    <p>Chưa có thông tin</p>
}
<p>
    @if (User.Identity.IsAuthenticated)
    {<a asp-action="Create">Đăng tin</a>}
</p>
<div class="col-md-12">
    <div class="well well-lg offer-box text-center" style="">        
        @await Html.PartialAsync("_SearchBoxPartial")
    </div>
    <div class="main box-border">        
        @await Html.PartialAsync("_SliderPartial")
    </div>
    <br>
</div>
@await Html.PartialAsync("_ItemsListPartial")
<script>


</script>
